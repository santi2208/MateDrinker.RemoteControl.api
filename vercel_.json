{
    "version": 2,
    "builds": [
        {
            "src": "src/api/auth-user/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/api/commands/index-commands.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/login(.*)",
            "dest": "/src/api/auth-user/index.js"
        },
        {
            "src": "/api/v1/users(.*)",
            "dest": "/src/api/auth-user/index.js"
        },
        {
            "src": "/api/v1/commands(.*)",
            "dest": "src/api/commands/index-commands.js"
        }
    ]
}